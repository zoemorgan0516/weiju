<p id="notice"><%= notice %></p>

<h1>Wechatinfos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wechatinfos.each do |wechatinfo| %>
      <tr>
        <td><%= link_to 'Show', wechatinfo %></td>
        <td><%= link_to 'Edit', edit_wechatinfo_path(wechatinfo) %></td>
        <td><%= link_to 'Destroy', wechatinfo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wechatinfo', new_wechatinfo_path %>
